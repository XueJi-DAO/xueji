{
  "name": "xueji",
  "version": "0.0.1",
  "license": "MIT",
  "author": "AaronZZH",
  "keywords": [
    "React",
    "TypeScript",
    "Neo4j",
    "Graphql",
    "Recoil"
  ],
  "scripts": {
    "codegen": "graphql-codegen -w",
    "generate": "prisma generate --schema=./apps/xueji/prisma/schema.prisma",
    "migrate-dev": "prisma migrate dev --schema=./apps/xueji/prisma/schema.prisma",
    "seed": "ts-node tools/seed.ts",
    "gen-schema": "ts-node tools/neo4j",
    "prepare": "husky install",
    "build-rust": "rustc --target wasm32-unknown-unknown -O --crate-type=cdylib apps/xueji/wasm/add.rs -o apps/xueji/public/add.wasm"
  },
  "private": true,
