    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^4.16.2",
    "@react-three/drei": "^9.78.2",
    "@react-three/fiber": "^8.13.5",
    "@swc/helpers": "~0.5.1",
    "clsx": "^1.2.1",
    "deepmerge": "^4.3.1",
    "framer-motion": "^10.12.20",
    "graphql": "^16.7.1",
    "neo4j-driver": "^5.10.0",
    "next": "^13.4.9",
