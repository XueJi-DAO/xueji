{
  "name": "@xueji/web",
  "version": "0.0.1",
  "license": "MIT",
  "author": "AaronZZH",
  "keywords": [
    "React",
    "TypeScript",
    "Neo4j",
    "Graphql",
    "Recoil"
  ],
  "scripts": {
    "generate": "dotenv -e .env.local -- pnpm dlx prisma generate --schema=apps/xueji/src/lib/prisma/schema.prisma",
    "migrate-dev": "dotenv -e .env.local -- pnpm dlx prisma migrate dev --schema=./apps/xueji/src/lib/prisma/schema.prisma",
    "migrate-deploy": "dotenv -e .env.local -- pnpm dlx prisma migrate deploy --schema=./apps/xueji/src/lib/prisma/schema.prisma",
    "codegen": "graphql-codegen -w",
    "seed": "dotenv -e .env.local ts-node tools/seed.ts",
    "gen-schema": "ts-node tools/neo4j",
    "prepare": "husky",
    "build-rust": "rustc --target wasm32-unknown-unknown -O --crate-type=cdylib apps/xueji/wasm/add.rs -o apps/xueji/public/add.wasm",
    "analyze": "cross-env ANALYZE=true nx build xueji",
    "serve": "serve .next/analyze",
    "lint": "nx run-many -t lint"
  },
  "private": true,
  "engines": {
    "node": ">=20.14.0",
    "pnpm": "^9.9.0"
  },
  "packageManager": "pnpm@9.9.0",
