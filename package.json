    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "recoil": "^0.7.7",
    "server-only": "^0.0.1",
    "swr": "^2.2.0",
    "three": "^0.152.2",
    "tslib": "^2.6.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.6",
