{
  "editor.tabSize": 2,
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit"
  },
  "cSpell.words": [
    "ahooks",
    "apexcharts",
    "apisix",
    "autoprefixer",
    "cdylib",
    "chartjs",
    "classname",
    "clevision",
    "clsx",
    "codegen",
    "commitlint",
    "csstype",
    "Cypher",
    "datasource",
    "datepicker",
    "daygrid",
    "deepmerge",
    "draftjs",
    "drei",
    "fontsource",
    "fullcalendar",
    "GLTF",
    "gsap",
    "heroicons",
    "hookform",
    "iconify",
    "impressjs",
    "introspector",
    "keycloak",
    "languagedetector",
    "minipass",
    "nextauth",
    "nprogress",
    "ollama",
    "openlayers",
    "papayawhip",
    "PDCA",
    "popperjs",
    "prismjs",
    "Quarkus",
    "reduxjs",
    "roboto",
    "rustc",
    "signin",
    "signout",
    "signup",
    "slatejs",
    "snsapi",
    "stylis",
    "tailwindcss",
    "threejs",
    "timegrid",
    "todos",
    "trpc",
    "tsparticles",
    "twojs",
    "webui",
    "xueji"
  ],
  "typescript.tsdk": "node_modules\\typescript\\lib",
  "typescript.enablePromptUseWorkspaceTsdk": true,
  // 折叠显示
  "explorer.fileNesting.enabled": true,
  "explorer.fileNesting.expand": false,
  "explorer.fileNesting.patterns": {
    "*.ts": "$(capture).test.ts, $(capture).test.tsx",
    "*.tsx": "$(capture).test.ts, $(capture).test.tsx",
    "*.env": "$(capture).env.*",
    "package.json": "docker-compose.*,app.json,pnpm-lock.yaml,LICENSE,CHANGELOG*,CNAME,.gitattributes,.eslintrc-auto-import.json,.gitignore,.prettierrc,stylelint.config.js,commitlint.config.js,.prettierignore,.eslintrc.json,.eslintignore,.editorconfig,code_of_conduct.md,.npmrc,.dockerignore,Dockerfile,jest.config.ts,jest.preset.js"
  },
  "i18n-ally.localesPaths": ["apps/xueji/messages", "apps/xueji/src/i18n"]
}
